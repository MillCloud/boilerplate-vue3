{
  "name": "boilerplate-vue3",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "check:deps": "ncu",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue,svelte}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it && pnpm run build",
    "serve": "vite preview -- port 5050"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.7",
    "@iconify/vue": "^3.1.3",
    "@modyqyw/utils": "^2.1.2",
    "@vueuse/core": "^7.6.2",
    "@vueuse/integrations": "^7.6.2",
    "@vueuse/router": "^7.6.2",
    "axios": "^0.26.0",
    "browser-update": "^3.3.38",
    "color": "^4.2.1",
    "dayjs": "^1.10.7",
    "element-plus": "^2.0.2",
    "element-pro-components": "^0.18.0",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.11",
    "query-string": "^7.1.1",
    "statuses": "^2.0.1",
    "vue": "^3.2.31",
    "vue-query": "^1.19.1",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/prompt": "^16.2.1",
    "@iconify/json": "^2.1.6",
    "@modyqyw/fabric": "^4.12.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/browser-update": "^3.3.0",
    "@types/color": "^3.0.3",
    "@types/nprogress": "^0.2.0",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.3.4",
    "markdownlint-cli": "^0.31.1",
    "npm-check-updates": "^12.4.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "release-it": "^14.12.5",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "shelljs": "^0.8.5",
    "stylelint": "^14.5.3",
    "tailwindcss": "^3.0.23",
    "type-fest": "^2.12.0",
    "typescript": "^4.5.5",
    "unplugin-icons": "^0.13.2",
    "unplugin-vue-components": "^0.17.21",
    "unplugin-vue-define-options": "^0.3.1",
    "vite": "^2.8.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-mkcert": "^1.5.2",
    "vite-plugin-pages": "^0.20.2",
    "vite-plugin-stylelint": "^1.1.3",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue-tsc": "^0.32.0"
  }
}
