{
  "name": "boilerplate-vue3",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "scripts": {
    "build": "vue-tsc && vite build",
    "check:deps": "ncu",
    "check:types": "vue-tsc",
    "clean": "rimraf ./node_modules/.vite && rimraf ./dist",
    "commit": "cz",
    "dev": "vite",
    "postinstall": "pnpm run clean",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it && pnpm run build",
    "serve": "vite preview -- port 5050"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@element-plus/icons-vue": "~0.2.4",
    "@iconify/vue": "~3.1.1",
    "@modyqyw/utils": "~2.0.1",
    "@vueuse/core": "~7.4.0",
    "@vueuse/integrations": "~7.4.0",
    "@vueuse/router": "~7.4.0",
    "axios": "~0.24.0",
    "dayjs": "~1.10.7",
    "element-plus": "~1.2.0-beta.6",
    "modern-normalize": "~1.1.0",
    "nprogress": "~0.2.0",
    "pinia": "~2.0.8",
    "query-string": "~7.0.1",
    "statuses": "~2.0.1",
    "vue": "~3.2.13",
    "vue-query": "~1.14.0",
    "vue-router": "~4.0.12"
  },
  "devDependencies": {
    "@babel/core": "~7.16.5",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~15.0.0",
    "@iconify/json": "~1.1.445",
    "@modyqyw/fabric": "~4.6.0",
    "@tailwindcss/aspect-ratio": "~0.4.0",
    "@tailwindcss/forms": "~0.4.0",
    "@tailwindcss/line-clamp": "~0.3.0",
    "@tailwindcss/typography": "~0.5.0",
    "@types/nprogress": "~0.2.0",
    "@types/statuses": "~2.0.0",
    "@typescript-eslint/eslint-plugin": "~5.8.0",
    "@typescript-eslint/parser": "~5.8.0",
    "@vitejs/plugin-vue": "~2.0.1",
    "@vitejs/plugin-vue-jsx": "~1.3.3",
    "commitizen": "~4.2.4",
    "cross-env": "~7.0.3",
    "eslint": "~8.5.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.3",
    "markdownlint-cli": "~0.30.0",
    "npm-check-updates": "~12.0.5",
    "picocolors": "~1.0.0",
    "postcss-preset-env": "~7.0.2",
    "prettier": "~2.5.1",
    "release-it": "~14.11.8",
    "rimraf": "~3.0.2",
    "sass": "~1.45.1",
    "shelljs": "~0.8.4",
    "stylelint": "~14.2.0",
    "tailwindcss": "~3.0.7",
    "typescript": "~4.5.4",
    "unplugin-icons": "~0.12.23",
    "unplugin-vue-components": "~0.17.9",
    "vite": "~2.7.5",
    "vite-plugin-env-compatible": "~1.1.1",
    "vite-plugin-eslint": "~1.3.0",
    "vite-plugin-mkcert": "~1.5.2",
    "vite-plugin-pages": "~0.19.6",
    "vite-plugin-stylelint": "~1.1.0",
    "vite-plugin-vue-layouts": "~0.5.0",
    "vue-tsc": "~0.30.0"
  }
}
