{
  "name": "boilerplate-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "commit": "cz",
    "dev": "vite",
    "lint": "yarn run lint:json && yarn run lint:markdown && yarn run lint:script && yarn run lint:style",
    "lint:json": "prettier ./**/*.json --write --ignore-path=.gitignore",
    "lint:markdown": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:script": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:style": "stylelint ./**/*.{css,less,sass,scss,vue} --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "serve": "vite preview"
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@iconify/vue": "~3.0.0-beta.1",
    "@modyqyw/utils": "~0.7.0",
    "@vueuse/core": "~5.2.0",
    "@vueuse/gesture": "~1.0.0",
    "@vueuse/integrations": "~5.2.0",
    "@vueuse/motion": "~1.5.6",
    "@vueuse/router": "~5.2.0",
    "@windicss/animations": "~1.0.8",
    "@windicss/plugin-icons": "~1.0.2",
    "@windicss/plugin-question-mark": "~0.1.1",
    "@windicss/plugin-scrollbar": "~1.2.3",
    "ant-design-vue": "~2.2.2",
    "vue": "~3.1.5",
    "vue-i18n": "~9.1.7",
    "vue-router": "~4.0.10",
    "windicss": "~3.1.6"
  },
  "devDependencies": {
    "@babel/core": "~7.14.8",
    "@babel/eslint-parser": "~7.14.7",
    "@commitlint/cli": "~13.1.0",
    "@iconify/json": "~1.1.380",
    "@intlify/vite-plugin-vue-i18n": "~2.4.0",
    "@modyqyw/fabric": "~2.9.0",
    "@types/node": "~16.4.7",
    "@typescript-eslint/eslint-plugin": "~4.28.5",
    "@typescript-eslint/parser": "~4.28.5",
    "@vitejs/plugin-legacy": "~1.5.0",
    "@vitejs/plugin-vue": "~1.3.0",
    "@vue/compiler-sfc": "~3.1.5",
    "@vuedx/typescript-plugin-vue": "~0.7.4",
    "eslint": "~7.31.0",
    "husky": "~7.0.1",
    "is-ci": "~3.0.0",
    "lint-staged": "~11.1.1",
    "markdownlint-cli": "~0.28.1",
    "prettier": "~2.3.2",
    "release-it": "~14.10.1",
    "stylelint": "~13.13.1",
    "typescript": "~4.3.5",
    "vite": "~2.4.4",
    "vite-plugin-components": "~0.13.2",
    "vite-plugin-icons": "~0.6.5",
    "vite-plugin-pages": "~0.15.1",
    "vite-plugin-vue-layouts": "~0.3.1",
    "vite-plugin-windicss": "~1.2.7",
    "vite-tsconfig-paths": "~3.3.13",
    "vue-tsc": "~0.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
