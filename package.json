{
  "name": "boilerplate-vue3",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "scripts": {
    "build": "vue-tsc && vite build",
    "check:deps": "ncu",
    "check:types": "vue-tsc",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue,svelte}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it && pnpm run build",
    "serve": "vite preview -- port 5050"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@element-plus/icons-vue": "~0.2.4",
    "@iconify/vue": "~3.1.2",
    "@modyqyw/utils": "~2.1.2",
    "@vueuse/core": "~7.5.3",
    "@vueuse/integrations": "~7.5.3",
    "@vueuse/router": "~7.5.3",
    "axios": "~0.24.0",
    "browser-update": "~3.3.37",
    "dayjs": "~1.10.7",
    "element-plus": "~1.3.0-beta.5",
    "element-pro-components": "~0.17.1",
    "modern-normalize": "~1.1.0",
    "nprogress": "~0.2.0",
    "pinia": "~2.0.9",
    "query-string": "~7.1.0",
    "statuses": "~2.0.1",
    "vue": "~3.2.27",
    "vue-query": "~1.17.0",
    "vue-router": "~4.0.12"
  },
  "devDependencies": {
    "@babel/core": "~7.16.7",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~16.1.0",
    "@iconify/json": "~1.1.457",
    "@modyqyw/fabric": "~4.8.0",
    "@tailwindcss/aspect-ratio": "~0.4.0",
    "@tailwindcss/forms": "~0.4.0",
    "@tailwindcss/line-clamp": "~0.3.1",
    "@tailwindcss/typography": "~0.5.0",
    "@types/browser-update": "~3.3.0",
    "@types/nprogress": "~0.2.0",
    "@types/statuses": "~2.0.0",
    "@typescript-eslint/eslint-plugin": "~5.9.1",
    "@typescript-eslint/parser": "~5.9.1",
    "@vitejs/plugin-vue": "~2.0.1",
    "@vitejs/plugin-vue-jsx": "~1.3.3",
    "commitizen": "~4.2.4",
    "cross-env": "~7.0.3",
    "eslint": "~8.7.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.7",
    "markdownlint-cli": "~0.30.0",
    "npm-check-updates": "~12.1.0",
    "picocolors": "~1.0.0",
    "postcss-flexbugs-fixes": "~5.0.2",
    "postcss-preset-env": "~7.2.3",
    "prettier": "~2.5.1",
    "release-it": "~14.12.3",
    "rimraf": "~3.0.2",
    "sass": "~1.48.0",
    "shelljs": "~0.8.5",
    "stylelint": "~14.2.0",
    "tailwindcss": "~3.0.15",
    "type-fest": "~2.9.0",
    "typescript": "~4.5.4",
    "unplugin-icons": "~0.13.0",
    "unplugin-vue-components": "~0.17.13",
    "vite": "~2.7.12",
    "vite-plugin-env-compatible": "~1.1.1",
    "vite-plugin-eslint": "~1.3.0",
    "vite-plugin-mkcert": "~1.5.2",
    "vite-plugin-pages": "~0.20.0",
    "vite-plugin-stylelint": "~1.1.0",
    "vite-plugin-vue-layouts": "~0.5.0",
    "vue-tsc": "0.30.2"
  }
}
