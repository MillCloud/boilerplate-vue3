{
  "name": "boilerplate-vue",
  "version": "0.0.0",
  "private": true,
  "author": "MillCloud",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "commit": "cz",
    "dev": "vite",
    "lint": "pnpm run lint:json && pnpm run lint:markdown && pnpm run lint:script && pnpm run lint:style",
    "lint:json": "prettier ./**/*.json --write --ignore-path=.gitignore",
    "lint:markdown": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:script": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:style": "stylelint ./**/*.{css,less,scss,vue} --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "serve": "vite preview"
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@iconify/vue": "~3.0.0",
    "@modyqyw/utils": "~0.7.0",
    "@vueuse/core": "~6.0.0",
    "@vueuse/gesture": "~1.0.0",
    "@vueuse/integrations": "~6.0.0",
    "@vueuse/motion": "~1.6.0",
    "@vueuse/router": "~6.0.0",
    "ant-design-vue": "~2.2.6",
    "axios": "~0.21.1",
    "axios-logger": "~2.5.0",
    "axios-mock-adapter": "~1.20.0",
    "axios-retry": "~3.1.9",
    "dayjs": "~1.10.6",
    "mockjs": "~1.1.0",
    "nprogress": "~0.2.0",
    "statuses": "~2.0.1",
    "swrv": "~1.0.0-beta.8",
    "vue": "~3.2.3",
    "vue-i18n": "~9.1.7",
    "vue-query": "~1.10.0",
    "vue-router": "~4.0.11",
    "windicss": "~3.1.7"
  },
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/eslint-parser": "~7.15.0",
    "@commitlint/cli": "~13.1.0",
    "@iconify/json": "~1.1.391",
    "@intlify/vite-plugin-vue-i18n": "~2.4.0",
    "@modyqyw/fabric": "~3.0.3",
    "@types/jest": "~27.0.1",
    "@types/node": "~16.7.1",
    "@types/nprogress": "~0.2.0",
    "@types/statuses": "~2.0.0",
    "@typescript-eslint/eslint-plugin": "~4.29.2",
    "@typescript-eslint/parser": "~4.29.2",
    "@vitejs/plugin-legacy": "~1.5.1",
    "@vitejs/plugin-vue": "~1.4.0",
    "@vue/compiler-sfc": "~3.2.4",
    "@vuedx/typescript-plugin-vue": "~0.7.4",
    "@windicss/plugin-animations": "~1.0.9",
    "@windicss/plugin-heropatterns": "~0.0.8",
    "@windicss/plugin-interaction-variants": "~1.0.0",
    "@windicss/plugin-question-mark": "~0.1.1",
    "@windicss/plugin-scrollbar": "~1.2.3",
    "commitizen": "~4.2.4",
    "eslint": "~7.32.0",
    "husky": "~7.0.1",
    "is-ci": "~3.0.0",
    "lint-staged": "~11.1.2",
    "markdownlint-cli": "~0.28.1",
    "prettier": "~2.3.2",
    "release-it": "~14.11.5",
    "stylelint": "~13.13.1",
    "typescript": "~4.3.5",
    "vite": "~2.5.0",
    "vite-plugin-components": "~0.13.2",
    "vite-plugin-eslint": "~1.3.0",
    "vite-plugin-icons": "~0.6.5",
    "vite-plugin-pages": "~0.17.4",
    "vite-plugin-style-import": "~1.1.1",
    "vite-plugin-vue-layouts": "~0.3.1",
    "vite-plugin-windicss": "~1.2.7",
    "vite-tsconfig-paths": "~3.3.13",
    "vue-tsc": "~0.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "^12.22.5 || ^14.17.5 || ^16.6.2",
    "npm": "^6.14.14 || ^7.20.3"
  }
}
